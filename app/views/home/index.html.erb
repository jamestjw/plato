 <% if false %>
 <% @boards.each do |board| %>
    <p>
    <strong>Name:</strong>
    <%= board.name %>
    </p>

    <p>
    <strong>Active:</strong>
    <%= board.active %>
    </p>

    <p>
    <strong>Creator:</strong>
    <%= board.creator_id %>
    </p>

    <h5>Cards:</h5>
    <%= render board %>
<% end %>
<% end %>

<div class="deck">
    <%= render partial: 'boards/board_home', collection: @boards %>
</div>
